@model List<FrontEnd.Models.ViewModels.ActaHallazgoViewModel>

<table class="table">

    <thead class="thead-light">
        <tr>
            <th scope="col">Numero Folio</th>
            <th scope="col">Nombre Policia Encargado</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@Html.DisplayFor(modelItem => item.NumeroFolio)</th>
                <td>
                    <label><a href="#" onclick="reply_click2(this.id ,'@item.Inventario')" id="@item.NumeroFolio"> @item.VistaPoliciaEncargado</a></label>
                </td>
            </tr>
}
    </tbody>
</table>
