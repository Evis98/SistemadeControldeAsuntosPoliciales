@model List<FrontEnd.Models.ViewModels.ActaDecomisoViewModel>

<table class="table">

    <thead class="thead-light">
        <tr>
            <th scope="col">Número de Folio</th>
            <th scope="col">Fecha</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@Html.DisplayFor(modelItem => item.NumeroFolio)</th>
                <td>
                    <label><a href="#" onclick="reply_click4(this.id, this.name)" id="@item.NumeroFolio" name="@item.Inventario"> @item.Fecha</a></label>
                </td>
            </tr>
}
    </tbody>
</table>
